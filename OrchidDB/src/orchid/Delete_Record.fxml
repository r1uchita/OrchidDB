            <Tooltip text="Drag For Table Names" />
         </tooltip></JFXComboBox>
      <TableView fx:id="table_data" layoutX="42.0" layoutY="189.0" prefHeight="411.0" prefWidth="816.0">
         <tooltip>
            <Tooltip text="Depiction Of Tables" />
         </tooltip></TableView>
      <JFXButton fx:id="delete_button" buttonType="RAISED" layoutX="388.0" layoutY="622.0" onAction="#record_delete" prefHeight="31.0" prefWidth="196.0" styleClass="green" stylesheets="@button_css.css" text="Delete ">
         <font>
            <Font name="Times New Roman Bold Italic" size="23.0" />
