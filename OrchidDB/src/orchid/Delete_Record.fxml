<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="orchid.Delete_Record">
   <children>
      <JFXToggleButton layoutX="705.0" layoutY="14.0" prefHeight="60.0" prefWidth="81.0">
         <font>
            <Font name="System Bold" size="13.0" />
