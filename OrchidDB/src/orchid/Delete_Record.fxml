         <tooltip>
            <Tooltip text="Them Changer" />
         </tooltip>
      </JFXToggleButton>
      <JFXComboBox fx:id="table_names" layoutX="52.0" layoutY="97.0" prefHeight="40.0" prefWidth="272.0" promptText="Select Table">
